{
  "manifest_version": 2,

  "name": "iNCognito",
  "description": "This extension protects against CSRF attacks by virtually isolating cookies.",
  "version": "1.0.38",
  "default_locale": "en",
  "author": "Alex Everett, Tyler Stocksdale of NCSU",
  "icons": { "16": "small_icon.png", "48": "medium_icon.png", "128": "large_icon.png" },
  "browser_action": {
    "default_icon": "small_icon.png"
  },
  "permissions": [
    "activeTab",
    "http://*/",
    "https://*/",
    "tabs",
    "cookies",
    "background",
    "webRequest",
    "webRequestBlocking"
  ],
    "background": {
        "scripts": ["incognito.js"]
    }
}
